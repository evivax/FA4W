  <method CLSNAME="ZFAW_BSP_DLC__GRIDCONADV_IMPL" CMPNAME="ON_AFTER_SAVE" VERSION="1" LANGU="D" DESCRIPT="On After Save" EXPOSURE="1" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="1" MTDDECLTYP="0" REFCLSNAME="CL_BSP_DLC_VIEW_CONTROLLER" REFCMPNAME="AFTER_SAVE" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD on_after_save.
    DATA lv_context_id          TYPE bsp_dlc_context_id.
    DATA lt_modfield TYPE TABLE OF zpa_bsp_modfield.
    DATA ls_modfield LIKE LINE OF  lt_modfield .

    FIELD-SYMBOLS &lt;fs_zbsp_field_probs&gt; LIKE LINE OF zbsp_field_probs.

    cl_bsp_wd_context_api2=&gt;get_context_id(
       EXPORTING
         iv_component       = config_core-&gt;config-&gt;gs_config_key-component
         iv_viewname        = config_core-&gt;config-&gt;gs_config_key-viewname
         iv_role_key        = config_core-&gt;config-&gt;gs_config_key-role_key
         iv_component_usage = config_core-&gt;config-&gt;gs_config_key-component_usage
         iv_object_type     = config_core-&gt;config-&gt;gs_config_key-object_type
         iv_object_sub_type = config_core-&gt;config-&gt;gs_config_key-object_sub_type
       RECEIVING
         rv_context_id      = lv_context_id
       EXCEPTIONS
         parameter_error    = 1
         not_found          = 2
         OTHERS             = 3
            ).
    IF sy-subrc &lt;&gt; 0.
* Implement suitable error handling here
    ENDIF.

    LOOP AT zbsp_field_probs ASSIGNING &lt;fs_zbsp_field_probs&gt;.
      CLEAR ls_modfield.

      ls_modfield-mandt = sy-mandt.
      ls_modfield-context_id = lv_context_id.
      ls_modfield-field_name = &lt;fs_zbsp_field_probs&gt;-field_name.
      ls_modfield-field_group = &lt;fs_zbsp_field_probs&gt;-field_group.

      APPEND ls_modfield TO lt_modfield.
    ENDLOOP.

    MODIFY zfaw_field FROM TABLE lt_modfield.

    IF zbsp_view-auth_objct IS INITIAL OR zbsp_view-auth_objct = &apos;&apos;.
      DELETE FROM zfaw_view WHERE context_id = lv_context_id.
    ELSE.
      MODIFY zfaw_view FROM zbsp_view.
    ENDIF.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_BSP_DLC__GRIDCONADV_IMPL" CMPNAME="OP_TOPOPUPVIEWAUTHOBJ" VERSION="1" LANGU="D" EXPOSURE="1" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_BSP_DLC__GRIDCONADV_IMPL" CMPNAME="OP_TOPOPUPVIEWAUTHOBJ" SCONAME="IV_DATA_COLLECTION" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="0 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="IF_BOL_BO_COL" PAROPTIONL="X"/>
   <source>METHOD op_topopupviewauthobj.
* Added by wizard: Outbound plug &apos;TOPOPUPVIEWAUTHOBJ&apos;
    IF product_popup IS NOT BOUND.
      product_popup = comp_controller-&gt;window_manager-&gt;create_popup(
                      iv_interface_view_name = &apos;ZFAW_BSP_DLC_CN/WindowPopUpViewAuthObj&apos;
                      iv_usage_name          = &apos;CuPopUpViewAuthObj&apos;
                      iv_title               = &apos;PopUp by Pascal&apos;
                  ).
    ENDIF.

    DATA lv_context_id          TYPE bsp_dlc_context_id.
    DATA lr_context_node TYPE REF TO cl_bsp_wd_context_node.

    cl_bsp_wd_context_api2=&gt;get_context_id(
      EXPORTING
        iv_component       = config_core-&gt;config-&gt;gs_config_key-component
        iv_viewname        = config_core-&gt;config-&gt;gs_config_key-viewname
        iv_role_key        = config_core-&gt;config-&gt;gs_config_key-role_key
        iv_component_usage = config_core-&gt;config-&gt;gs_config_key-component_usage
        iv_object_type     = config_core-&gt;config-&gt;gs_config_key-object_type
        iv_object_sub_type = config_core-&gt;config-&gt;gs_config_key-object_sub_type
      RECEIVING
        rv_context_id      = lv_context_id
      EXCEPTIONS
        parameter_error    = 1
        not_found          = 2
        OTHERS             = 3
           ).
    IF sy-subrc &lt;&gt; 0.
* Implement suitable error handling here
    ENDIF.


    IF zbsp_view IS INITIAL.

      SELECT SINGLE *  FROM zfaw_view INTO zbsp_view WHERE context_id = lv_context_id.
    ENDIF.

    lr_context_node = product_popup-&gt;get_context_node( iv_cnode_name = &apos;VIEWAUTHOBJNAME&apos; ).

    lr_context_node-&gt;collection_wrapper-&gt;get_current( )-&gt;set_property( iv_attr_name = &apos;AUTHOBJ&apos; iv_value = zbsp_view-auth_objct ).

    product_popup-&gt;set_window_width( iv_width = 300 ).
    product_popup-&gt;set_on_close_event( iv_view = me iv_event_name = &apos;VIEWAUTHOBJNAME_CLOSE&apos; ).
    product_popup-&gt;set_display_mode( if_bsp_wd_popup=&gt;c_display_mode_surrounded ).

    product_popup-&gt;open( ).
  ENDMETHOD.</source>
  </method>
