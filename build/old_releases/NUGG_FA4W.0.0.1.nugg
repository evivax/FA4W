  <method CLSNAME="CL_BSP_DLC__GRIDCONADV_IMPL" CMPNAME="DO_HANDLE_EVENT" VERSION="0" EXPOSURE="0" STATE="0" EDITORDER="0 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD do_handle_event.
* Eventhandler dispatching
    global_event = super-&gt;do_handle_event( event           = event
                                       htmlb_event     = htmlb_event
                                       htmlb_event_ex  = htmlb_event_ex
                                       global_messages = global_messages ).

    DATA lv_fieldname           TYPE bsp_dlc_binding_string.
    DATA ls_field_probs         LIKE LINE OF zbsp_field_probs.
    DATA lv_context_id          TYPE bsp_dlc_context_id.


    CASE htmlb_event_ex-&gt;event_server_name.
*     Added by wizard
      WHEN &apos;BTN_POPUPVIEWAUTHOBJ&apos;.                          &quot;#EC NOTEXT
        eh_onbtn_popupviewauthobj( htmlb_event    = htmlb_event
                    htmlb_event_ex = htmlb_event_ex ).

*     Added by wizard
      WHEN &apos;POPUPVIEWAUTHOBJ_CLOSE&apos;.                        &quot;#EC NOTEXT
        eh_onpopupviewauthobj_close( htmlb_event    = htmlb_event
                    htmlb_event_ex = htmlb_event_ex ).

      WHEN &apos;SHOW_FIELD_PROPS&apos;.

        lv_fieldname = typed_context-&gt;gridelements-&gt;get_field_name( &apos;&apos; ).

        cl_bsp_wd_context_api2=&gt;get_context_id(
          EXPORTING
            iv_component       = config_core-&gt;config-&gt;gs_config_key-component
            iv_viewname        = config_core-&gt;config-&gt;gs_config_key-viewname
            iv_role_key        = config_core-&gt;config-&gt;gs_config_key-role_key
            iv_component_usage = config_core-&gt;config-&gt;gs_config_key-component_usage
            iv_object_type     = config_core-&gt;config-&gt;gs_config_key-object_type
            iv_object_sub_type = config_core-&gt;config-&gt;gs_config_key-object_sub_type
          RECEIVING
            rv_context_id      = lv_context_id
          EXCEPTIONS
            parameter_error    = 1
            not_found          = 2
            OTHERS             = 3
               ).
        IF sy-subrc &lt;&gt; 0.
* Implement suitable error handling here
        ENDIF.


        READ TABLE zbsp_field_probs WITH KEY field_name = lv_fieldname INTO ls_field_probs.
        IF sy-subrc &lt;&gt; 0.

          SELECT SINGLE *  FROM zfaw_field INTO CORRESPONDING FIELDS OF ls_field_probs WHERE context_id = lv_context_id AND field_name = lv_fieldname.
        ENDIF.
        ztyped_context-&gt;fieldgroup-&gt;get_collection_wrapper( )-&gt;get_current( )-&gt;set_property_as_string( iv_attr_name = &apos;FIELD_GROUP&apos; iv_value = ls_field_probs-field_group ).

      WHEN &apos;ApplyChanges&apos;.
        DATA lv_field_group TYPE string.

        lv_field_group = ztyped_context-&gt;fieldgroup-&gt;get_collection_wrapper( )-&gt;get_current( )-&gt;get_property_as_string( &apos;FIELD_GROUP&apos; ).

        ls_field_probs-field_name = typed_context-&gt;gridelements-&gt;get_field_name( &apos;&apos; ).
        ls_field_probs-field_group =  lv_field_group.

        READ TABLE zbsp_field_probs WITH KEY field_name = ls_field_probs-field_name TRANSPORTING NO FIELDS.
        IF sy-subrc = 0.

          MODIFY zbsp_field_probs FROM ls_field_probs TRANSPORTING field_group WHERE field_name = ls_field_probs-field_name.
