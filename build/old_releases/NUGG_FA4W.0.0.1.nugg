   <parameter CLSNAME="ZFAW_BSP_DLC__BSPWDCOMPON_CN00" CMPNAME="GET_M_AUTHOBJ" SCONAME="ATTRIBUTE_PATH" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <parameter CLSNAME="ZFAW_BSP_DLC__BSPWDCOMPON_CN00" CMPNAME="GET_M_AUTHOBJ" SCONAME="METADATA" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="3" TYPE="IF_BSP_METADATA_SIMPLE"/>
   <source>method GET_M_AUTHOBJ.


    DATA: attr    TYPE XUOBJECT.

