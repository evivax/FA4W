  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DOM_TO_XML" VERSION="1" LANGU="D" EXPOSURE="0" STATE="1" EDITORDER="10 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD dom_to_xml.
    DATA lv_xstring_xml TYPE xstring.

    CALL FUNCTION &apos;SDIXML_DOM_TO_XML&apos;
      EXPORTING
        document      = me-&gt;document
      IMPORTING
        xml_as_string = lv_xstring_xml.


    CALL FUNCTION &apos;CRM_IC_XML_XSTRING2STRING&apos;
      EXPORTING
        inxstring = lv_xstring_xml
      IMPORTING
        outstring = me-&gt;xml.

    fix_xml_declaration( ).
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DO_ACTIVITY_BLINDOUT" VERSION="1" LANGU="D" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DO_ACTIVITY_BLINDOUT" SCONAME="IV_FIELD_NAME" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <source>METHOD do_activity_blindout.
    DATA lif_node                    TYPE REF TO if_ixml_node.
    DATA lif_ixml_node_label      TYPE REF TO if_ixml_node.

    DATA lif_tmp_current_node                     TYPE REF TO if_ixml_node.
    DATA lv_col                   TYPE        i.
    DATA lv_row                   TYPE        i.

    &quot; If there is no premission the field gets deleted
    &quot; Get the row and column value for empty_row check
    lv_col = me-&gt;current_node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;col&apos; )-&gt;get_value( ).
    lv_row = me-&gt;current_node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;get_value( ).
    &quot; Save the actual node in a tmp variable
    lif_node  = me-&gt;current_node.
    &quot; Get the previous node
    lif_ixml_node_label = me-&gt;current_node-&gt;get_prev( ).
    &quot; Get the next element in xml. Without this, the next element is not considered
    me-&gt;current_node = me-&gt;current_node-&gt;get_prev( ).
    &quot; Remove the field from xml
    lif_node-&gt;remove_node( ).
    &quot; Set the boolean that an field has removed
*    lv_field_removed = abap_true.
    &quot; Check if previous node is bind
    IF lif_ixml_node_label IS NOT INITIAL.
      &quot; Get the &apos;for&apos; attribut of the element. At a label element this contains  the element id which it is for
      lif_node = lif_ixml_node_label-&gt;get_first_child( )-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;for&apos; ).
      &quot; Check if the for Attribut is set and its the same then the current element id
      IF lif_node IS NOT INITIAL AND lif_node-&gt;get_value( ) = iv_field_name.
        me-&gt;current_node = lif_ixml_node_label-&gt;get_prev( ).
        &quot; Delete the Label from xml
        lif_ixml_node_label-&gt;remove_node( ).
      ENDIF.
    ENDIF.
    me-&gt;remove_empty_row(
        iv_col = lv_col
        iv_row = lv_row
           ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DO_ACTIVITY_DISPLAY" VERSION="1" LANGU="D" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DO_ACTIVITY_DISPLAY" SCONAME="IV_FIELD_NAME" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <source>METHOD do_activity_display.
    DATA lif_attribut             TYPE REF TO if_ixml_node.

    lif_attribut = me-&gt;current_node-&gt;get_first_child( )-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;disabled&apos; ).
    IF lif_attribut IS BOUND.
      lif_attribut-&gt;set_value( value = &apos;X&apos; ).
    ENDIF.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DO_ADVANCED_AUTHORITY_CHECK" VERSION="1" LANGU="D" EXPOSURE="2" STATE="1" EDITORDER="1 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="DO_ADVANCED_AUTHORITY_CHECK" SCONAME="RV_XML" VERSION="1" LANGU="D" DESCRIPT="String für XML-Speicherung" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="BSP_DLC_XML"/>
   <source>METHOD do_advanced_authority_check.
    DATA lv_success TYPE abap_bool.
    me-&gt;parse_xml( ).
    lv_success = me-&gt;get_grid_type( ).

    IF lv_success = abap_true.
      me-&gt;get_fields( ).
      me-&gt;check_ui_elements_authority( ).
      me-&gt;dom_to_xml( ).
    ENDIF.

    rv_xml = me-&gt;xml.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="FIX_XML_DECLARATION" VERSION="1" LANGU="D" DESCRIPT="Fix the XML Declaration" EXPOSURE="0" STATE="1" EDITORDER="9 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>method FIX_XML_DECLARATION.

****Here the modified XML is retrieved , but it is not ready as it has been encoded. We have to remove encoding and put it in standard format for rendering.

  DATA: lv_defidx    TYPE sy-fdpos,
        lv_clstagidx TYPE sy-fdpos.

  lv_defidx = -1.
  lv_clstagidx = 10000.
  IF me-&gt;xml CS &apos;&lt;?xml&apos;.                                     &quot;#EC NOTEXT
    lv_defidx = sy-fdpos.
    IF me-&gt;xml CS &apos;&gt;&apos;.
      lv_clstagidx = sy-fdpos + 1.
      IF lv_clstagidx &gt; lv_defidx.
        me-&gt;xml = me-&gt;xml+lv_clstagidx.
      ENDIF.
    ENDIF.
  ENDIF.
  CONCATENATE &apos;&lt;?xml version=&quot;1.0&quot;?&gt;&apos; me-&gt;xml INTO me-&gt;xml.   &quot;#EC NOTEXT
  endmethod.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="GET_ACTIVITY" VERSION="1" LANGU="D" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="GET_ACTIVITY" SCONAME="IV_FIELD_GROUP" VERSION="1" LANGU="D" DESCRIPT="Feldgruppenname" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="ZFAW_FIELD_GROUP"/>
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="GET_ACTIVITY" SCONAME="RT_ACTIVITY" VERSION="1" LANGU="D" DESCRIPT="Aktivität" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ZFAW_ACTIVITY"/>
   <source>METHOD get_activity.
    rt_activity = 3.
    AUTHORITY-CHECK OBJECT me-&gt;auth_object
             ID &apos;ZFAW_ACTVT&apos; FIELD &apos;02&apos;
             ID &apos;ZFAW_FIELD&apos; FIELD iv_field_group.
    IF sy-subrc = 0.
      rt_activity = 2.
    ENDIF.
    AUTHORITY-CHECK OBJECT me-&gt;auth_object
             ID &apos;ZFAW_ACTVT&apos; FIELD &apos;01&apos;
             ID &apos;ZFAW_FIELD&apos; FIELD iv_field_group.
    IF sy-subrc = 0.
      rt_activity = 1.
    ENDIF.
  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="GET_FIELDS" VERSION="1" LANGU="D" DESCRIPT="Get the Fields for the modified View" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>method GET_FIELDS.

  SELECT * FROM zfaw_field INTO TABLE me-&gt;fields_to_modify WHERE context_id = me-&gt;context_id.

  endmethod.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="GET_GRID_TYPE" VERSION="1" LANGU="D" EXPOSURE="0" STATE="1" EDITORDER="0 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="GET_GRID_TYPE" SCONAME="RV_SUCCESS" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="ABAP_BOOL"/>
   <source>METHOD get_grid_type.
    DATA lif_node      TYPE REF TO if_ixml_node.

    rv_success = abap_false.
*** Get Attribut &apos;gridtype&apos; from node &apos;grid&apos;

    lif_node =  me-&gt;document-&gt;get_first_child( )-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;gridtype&apos; ).

*** read the value of &apos;gridtype&apos; if node &apos;grid&apos; is bound
    IF lif_node IS BOUND.
      me-&gt;grid_type = lif_node-&gt;get_value( ).
      IF me-&gt;grid_type = cl_bsp_dlc_config_grid_based=&gt;type_16c1p
        OR me-&gt;grid_type = cl_bsp_dlc_config_grid_based=&gt;type_16c2p
        OR me-&gt;grid_type = cl_bsp_dlc_config_grid_based=&gt;type_8c1p.
        rv_success = abap_true.
      ENDIF.
    ENDIF.

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="PARSE_XML" VERSION="1" LANGU="D" DESCRIPT="Create DOM Document of XML" EXPOSURE="0" STATE="1" EDITORDER="8 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>METHOD parse_xml.

    DATA lif_ixml                 TYPE REF TO if_ixml.
    DATA lif_ixml_parser          TYPE REF TO if_ixml_parser.
    DATA lif_ixml_document        TYPE REF TO if_ixml_document.
    DATA lif_ixml_stream_factory  TYPE REF TO if_ixml_stream_factory.
    DATA lif_ixml_istream         TYPE REF TO if_ixml_istream.

**-- Create the Main Factory
    lif_ixml = cl_ixml=&gt;create( ).

**-- Create the Initial Document
    me-&gt;document = lif_ixml-&gt;create_document( ).

**-- Create a Stream Factory
    lif_ixml_stream_factory = lif_ixml-&gt;create_stream_factory( ).

**-- Create an Input Stream
    lif_ixml_istream       = lif_ixml_stream_factory-&gt;create_istream_string( string = me-&gt;xml ).

**-- Create a Parser
    lif_ixml_parser = lif_ixml-&gt;create_parser(
        document       = me-&gt;document
        istream        = lif_ixml_istream
        stream_factory = lif_ixml_stream_factory
    ).

    lif_ixml_parser-&gt;parse( ).

**-- Close the Input Stream after Parsing
    lif_ixml_istream-&gt;close( ).

  ENDMETHOD.</source>
  </method>
  <method CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="REMOVE_EMPTY_ROW" VERSION="1" LANGU="D" DESCRIPT="Removes empty row from xml" EXPOSURE="0" STATE="1" EDITORDER="7 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="REMOVE_EMPTY_ROW" SCONAME="IV_COL" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="I"/>
   <parameter CLSNAME="ZFAW_CL_CONFIGURATION2" CMPNAME="REMOVE_EMPTY_ROW" SCONAME="IV_ROW" VERSION="1" LANGU="D" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="I"/>
   <source>METHOD remove_empty_row.
    DATA lv_empty_row             TYPE        abap_bool.
    DATA node                     TYPE REF TO if_ixml_node.
*    DATA lif_node_gridcell            TYPE REF TO if_ixml_node.
*    Data iv_col                   type        i.
*    Data iv_row                   type        i.
    DATA lv_tmp_row               TYPE        i.
    DATA lv_tmp_col               TYPE        i.
    DATA lv_col_min               TYPE        i.
    DATA lv_col_max               TYPE        i.
    DATA lv_string                TYPE        string.

*   Get the width of the Grid Type. There are 3 types. Look at the static attributes
    lv_empty_row = abap_true.
    CASE me-&gt;grid_type.
      WHEN cl_bsp_dlc_config_grid_based=&gt;type_16c1p.
        lv_col_min = 1.
        lv_col_max = 16.
      WHEN cl_bsp_dlc_config_grid_based=&gt;type_8c1p.
        lv_col_min = 1.
        lv_col_max = 8.
      WHEN OTHERS.
        IF iv_col &lt;= 8.
          lv_col_min = 1.
          lv_col_max = 8.
        ELSE.
          lv_col_min = 9.
          lv_col_max = 16.
        ENDIF.
    ENDCASE.

* Get the row from the previous Entry in the Node-List
    node = me-&gt;current_node-&gt;get_prev( ).
    lv_tmp_row = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;get_value( ).

* Check all previous entries in the same row, whether they are in the same area
    WHILE lv_tmp_row = iv_row.
      lv_tmp_col = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;col&apos; )-&gt;get_value( ).
      IF lv_tmp_col &gt;= lv_col_min AND lv_tmp_col &lt;= lv_col_max.
        lv_empty_row = abap_false.
      ENDIF.
      node = node-&gt;get_prev( ).
      lv_tmp_row = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;get_value( ).
    ENDWHILE.

* Get the row from the Upcoming Entry in the Node-List
    node = me-&gt;current_node.
    lv_tmp_row = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;get_value( ).

* Check all Upcoming entries in the same row, whether they are in the same area
    WHILE lv_tmp_row = iv_row.
      lv_tmp_col = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;col&apos; )-&gt;get_value( ).
      IF lv_tmp_col &gt;= lv_col_min AND lv_tmp_col &lt;= lv_col_max.
        lv_empty_row = abap_false.
      ENDIF.
      node = node-&gt;get_next( ).
      lv_tmp_row = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;get_value( ).
    ENDWHILE.

* Check whether another entry in the field is present
    IF lv_empty_row = abap_true.
*      All of the following entries move one row up
      node = me-&gt;current_node.
      WHILE node IS NOT INITIAL.
        lv_tmp_row = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;get_value( ).
        lv_tmp_col = node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;col&apos; )-&gt;get_value( ).
        IF lv_tmp_row &gt; iv_row AND lv_tmp_col &gt;= lv_col_min AND lv_tmp_col &lt;= lv_col_max.
          lv_tmp_row = lv_tmp_row - 1.
          lv_string = lv_tmp_row.
          node-&gt;get_attributes( )-&gt;get_named_item_ns( name = &apos;row&apos; )-&gt;set_value( value = lv_string ).
        ENDIF.
        node = node-&gt;get_next( ).
      ENDWHILE.
    ENDIF.

  ENDMETHOD.</source>
  </method>
